{
  "token": {
    "label": "Token",
    "type": "Password",
    "required": true,
    "help": "Please provide your Splunk Token.",
    "maxLength": 100
  },
  "url": {
    "label": "URL",
    "type": "String",
    "required": true,
    "help": "Please provide your Splunk instance URL.",
    "maxLength": 50,
    "format": {
      "pattern": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\-]*[a-zA-Z0-9])\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\-]*[A-Za-z0-9])$"
    }
  }
}